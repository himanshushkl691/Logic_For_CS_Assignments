Section Pred_Logic_Assg2b.
  Variable S : Set.
  Variable P Q : S -> Prop.
  Hypothesis H1 : forall x : S,  P x -> Q x.
  Hypothesis H2 : exists x : S,  P x.
  Lemma ASSG2B_1 : exists x : S,  Q x.
  Proof.
    destruct H2 as [a H3].
    exists a.
    pose (H4 := H1 a).
    apply H4.
    exact H3.
  Qed.
  
